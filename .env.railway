# This file is a reference for Railway environment variables
# DO NOT commit the actual .env file with real API keys!
# Set these variables in Railway dashboard instead

# Required for Railway
RAILWAY_ENVIRONMENT=production
FLASK_ENV=production
BASE_URL=https://aiezzy.com

# Required API Keys (SET IN RAILWAY)
SECRET_KEY=generate-in-railway-or-use-python-secrets
OPENAI_API_KEY=your-key-here
FAL_KEY=your-key-here
TAVILY_API_KEY=your-key-here

# Optional - Email (SendGrid)
# SENDGRID_API_KEY=your-key-here
# SENDGRID_FROM_EMAIL=noreply@aiezzy.com
# EMAIL_VERIFICATION_REQUIRED=false

# Optional - OAuth
# GOOGLE_CLIENT_ID=your-id
# GOOGLE_CLIENT_SECRET=your-secret
# GITHUB_CLIENT_ID=your-id
# GITHUB_CLIENT_SECRET=your-secret

# Optional - Custom Quotas
# QUOTA_FREE_IMAGES=20
# QUOTA_FREE_VIDEOS=5
# QUOTA_FREE_MESSAGES=100

# DATABASE_URL is automatically set by Railway PostgreSQL plugin
